{
  "manifest_version": 2,

  "name": "Mystique",
  "description": "This extension generates random user profiles in order to confuse trackers",
  "version": "1.0",
  "options_page": "options.html",

  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
	"storage"
  ],
   "icons": {
    "128": "icon.png"
	}
}